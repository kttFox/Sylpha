<Window x:Class="$rootnamespace$.$safeitemname$"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:behaviors="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:l="http://schemas.sylpha.com/wpf/mvvm"
        xmlns:v="clr-namespace:$rootnamespace$"
        Title="$safeitemname$" Height="350" Width="525">
        xmlns:vm="clr-namespace:$rootnamespace$.ViewModels"

    <Window.DataContext>
        <vm:$safeitemname$ViewModel />
    </Window.DataContext>
    
    <behaviors:Interaction.Triggers>
        <behaviors:EventTrigger EventName="ContentRendered">
            <l:CallMethodAction MethodTarget="{Binding}" MethodName="Initialize" />
        </behaviors:EventTrigger>

        <behaviors:EventTrigger EventName="Closed">
            <l:DataContextDisposeAction />
        </behaviors:EventTrigger>
    </behaviors:Interaction.Triggers>

    <Grid>

    </Grid>
</Window>